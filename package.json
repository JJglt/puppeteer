        "mcp",
        "puppeteer",
        "template",
        "server",
        "node",
        "javascript"
    ],
    "author": "Russell Purinton <russell.purinton@gmail.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/rpurinton/puppeteer.git"
    },
    "bugs": {
        "url": "https://github.com/rpurinton/puppeteer/issues"
    },
    "license": "MIT",
    "dependencies": {
        "@modelcontextprotocol/sdk": "^1.12.1",
        "dotenv": "^16.5.0",
        "express": "^5.1.0",
        "puppeteer": "^24.10.0",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "jest": "^29.7.0",
        "supertest": "^7.1.1"
    },
    "jest": {
        "testEnvironment": "node",
        "testMatch": [
            "**/?(*.)+(spec|test).[jt]s?(x)",
